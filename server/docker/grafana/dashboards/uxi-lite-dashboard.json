  "id": 2,
  "uid": "uxi-lite",
  "title": "UXI-Lite",
  "schemaVersion": 36,
  "version": 20,
  "refresh": "30s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timezone": "browser",
  "tags": [
    "uxi",
    "wifi",
    "network-monitoring"
